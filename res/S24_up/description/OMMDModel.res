CONTAINER OMMDModel {
	NAME OMMDModel;
	INCLUDE Obase;

	GROUP MODEL_INFO_GRP {
		DEFAULT 1;

		REAL PMX_VERSION {
			ANIM OFF;
			UNIT REAL;
			MIN 2.0;
			STEP 0.1;
			MAX 2.1;
		}

		STRING MODEL_NAME_LOCAL {
			ANIM OFF;
		}

		STRING MODEL_NAME_UNIVERSAL {
			ANIM OFF;
		}

		STRING COMMENTS_LOCAL {
			ANIM OFF;
			CUSTOMGUI MULTISTRING;
			SCALE_H;
			SCALE_V;
		}

		STRING COMMENTS_UNIVERSAL {
			ANIM OFF;
			CUSTOMGUI MULTISTRING;
			SCALE_H;
			SCALE_V;
		}
	}

	GROUP MODEL_MORPH_GRP {
		GROUP MODEL_MORPH_GROUP_GRP {
			STRING MODEL_MORPH_GROUP_ADD_NAME {
				FIT_H;
				SCALE_H;
				ANIM OFF;
			}

			BUTTON MODEL_MORPH_GROUP_ADD_BUTTON {
				FIT_H;
				SCALE_H;
			}
		}

		GROUP MODEL_MORPH_FLIP_GRP {
			STRING MODEL_MORPH_FLIP_ADD_NAME {
				FIT_H;
				SCALE_H;
				ANIM OFF;
			}

			BUTTON MODEL_MORPH_FLIP_ADD_BUTTON {
				FIT_H;
				SCALE_H;
			}
		}

		GROUP MODEL_MORPH_MESH_GRP {
		}

		GROUP MODEL_MORPH_BONE_GRP {
		}
	}

	GROUP MODEL_FUNCTION_GRP {
		DEFAULT 1;

		BUTTON MODEL_DEL_BONE_ANIM_BUTTON {
			FIT_H;
			SCALE_H;
		}

		BUTTON MODEL_DEL_MORPH_ANIM_BUTTON {
			FIT_H;
			SCALE_H;
		}

		BUTTON MODEL_DEL_ALL_ANIM_BUTTON {
			FIT_H;
			SCALE_H;
		}
	}

	GROUP MODEL_IK_GRP {
	}
}
