CONTAINER OMMDRigidRoot {
	NAME OMMDRigidRoot;

	GROUP RIGID_ROOT_GRP {
		DEFAULT 1;


		LONG RIGID_DISPLAY_TYPE {
			CUSTOMGUI QUICKTABRADIO;
			ANIM OFF;
			FIT_H;
			SCALE_H;

			CYCLE {
				RIGID_DISPLAY_TYPE_OFF;
				RIGID_DISPLAY_TYPE_ON;
				RIGID_DISPLAY_TYPE_WIRE;
			}
		}

		LONG RIGID_MODE {
			CUSTOMGUI QUICKTABRADIO;
			ANIM OFF;
			FIT_H;
			SCALE_H;

			CYCLE {
				RIGID_MODE_EDIT;
				RIGID_MODE_ANIM;
			}
		}

		BUTTON ADD_RIGID_BUTTON {
			FIT_H;
			SCALE_H;
		}
	}
}
